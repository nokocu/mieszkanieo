{
    "name": "OLX.pl",
    "site_name": "olx", 
    "base_domain": "https://www.olx.pl",
    "base_url": "https://www.olx.pl/nieruchomosci/mieszkania/wynajem/{city}/?search[order]=filter_float_price:asc&view=grid&page=1",
