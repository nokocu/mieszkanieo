{
  "productName": "mieszkanieo",
  "identifier": "com.mieszkanieo.app",
  "version": "1.0.0",
  "build": {
    "beforeBuildCommand": "npm run frontend:build && npm run backend:build",
    "beforeDevCommand": "npm run frontend:dev && npm run backend:dev",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../frontend/dist"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "width": 1600,
        "height": 800,
        "resizable": true,
        "title": "Mieszkanieo",
        "devtools": true
      }
    ]
  },
  "bundle": {},
  "plugins": {}
}
